<h1 class="container">Create a new User</h1>
<h1>Login</h1>
<% if @errors %>
  <ul>
    <% @errors.each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>
<form class="container" method="post" action="/users">
  <input type="text" name="user[username]" placeholder="Username"><br>
  <input type="text" name="user[email]" placeholder="Email"><br>
  <input type="password" name="user[password]" placeholder="Password"><br>
  <input type="submit" value="submit">
</form>